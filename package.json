{
  "name": "@tapsellorg/semantic-release-general-config",
  "description": "semantic-release shareable config for Tapsell",
  "version": "1.0.0",
  "author": "Vahid Mohammadi",
  "bugs": {
    "url": "https://github.com/tapsellorg/semantic-release-general-config/issues"
  },
  "dependencies": {
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/gitlab": "^9.4.1",
    "@semantic-release/npm": "^9.0.1",
    "@semantic-release/release-notes-generator": "^10.0.3"
  },
  "devDependencies": {
    "semantic-release": "19.0.3"
  },
  "engines": {
    "node": ">=16"
  },
  "files": [
    "index.js"
  ],
  "homepage": "https://github.com/tapsellorg/semantic-release-general-config/",
  "keywords": [
    "gitlab",
    "conventional-changelog",
    "release",
    "semantic-release",
    "semantic-release-config",
    "version",
    "Tapasell"
  ],
  "license": "MIT",
  "main": "index.js",
  "peerDependencies": {
    "semantic-release": ">=18.0.0"
  },
  "prettier": {
    "printWidth": 120,
    "trailingComma": "es5"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tapsellorg/semantic-release-general-config.git"
  },
  "scripts": {
    "semantic-release": "semantic-release"
  },
  "renovate": {
    "extends": [
      "github>semantic-release/.github"
    ]
  }
}
